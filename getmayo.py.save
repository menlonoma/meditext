import urllib2
from bs4 import BeautifulSoup

url = 'https://www.google.com/search?q=migraine+treatment'
opener = urllib2.build_opener()
opener.addheaders = [('User-agent', 'Mozilla/5.0')]
html = opener.open(url).read()
soup = BeautifulSoup(html, 'html.parser')

target = open('scraped.txt', 'w')
target.write(soup)
target.close()

target2 = open('newscraped.txt', 'w')
target2.write(soup.prettify())
target2.close()

print soup.find_all("div", class_ = "_dyk _wje")

